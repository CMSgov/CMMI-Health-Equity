  "private": true,
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@trussworks/react-uswds": "^2.9.0",
    "@types/jest": "^27.4.0",
